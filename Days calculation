#include <stdio.h>

int main()
{
    int days,months,years;
    printf("Input no. of days: ");
    scanf("%d",&days);
    years = days /365;
    days = days % 365;
    months = days / 30;
    days = days %30;
    
    printf("%d Years\n",years);
    printf("%d month\n",months);
    printf("%d Day\n",days);

    return 0;
}
