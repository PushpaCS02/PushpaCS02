import random
def magic_8_ball():
    responses = [
        "Yes",
        "No",
        "Maybe",
        "Ask again later",
        "Definitely not",
        "Most likely",
        "Not sure",
        "Outlook good",
        "Cannot predict now",
        "Yes, in due time"
    ]
    question = input("Ask the Magic 8 Ball a yes/no question: ")
    answer = random.choice(responses)
    print(f"Magic 8 Ball says: {answer}")
magic_8_ball()
